<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="5dp"
        android:elevation="5dp"
        tools:ignore="MissingConstraints">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            tools:ignore="MissingConstraints">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="API Name"
                    android:textColor="#000000"
                    android:layout_weight=".30"
                    android:textSize="16dp"/>
                <TextView
                    android:id="@+id/apiName"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="name"
                    android:layout_weight="1"
                    android:textSize="16dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="URL"
                    android:textColor="#000000"
                    android:layout_weight=".30"
                    android:textSize="16dp"/>
                <TextView
                    android:id="@+id/apiURL"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="url"
                    android:layout_weight="1"
                    android:textSize="16dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Call Time"
                    android:textColor="#000000"
                    android:layout_weight=".30"
                    android:textSize="16dp"/>
                <TextView
                    android:id="@+id/callTime"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="time"
                    android:layout_weight="1"
                    android:textSize="16dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Request Body"
                    android:textColor="#000000"
                    android:layout_weight=".30"
                    android:textSize="16dp"/>
                <TextView
                    android:id="@+id/requestBody"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="body"
                    android:layout_weight="1"
                    android:textSize="16dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Response Code"
                    android:textColor="#000000"
                    android:layout_weight=".30"
                    android:textSize="16dp"/>
                <TextView
                    android:id="@+id/responseCode"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="code"
                    android:layout_weight="1"
                    android:textSize="16dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Response Body"
                    android:textColor="#000000"
                    android:layout_weight=".30"
                    android:textSize="16dp"/>
                <TextView
                    android:id="@+id/responseBody"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="body"
                    android:layout_weight="1"
                    android:textSize="16dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Exception"
                    android:textColor="#000000"
                    android:layout_weight=".30"
                    android:textSize="16dp"/>
                <TextView
                    android:id="@+id/exception"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="exception"
                    android:layout_weight="1"
                    android:textSize="16dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Response Time"
                    android:textColor="#000000"
                    android:layout_weight=".30"
                    android:textSize="16dp"/>
                <TextView
                    android:id="@+id/responseTime"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="time"
                    android:layout_weight="1"
                    android:textSize="16dp"/>
            </LinearLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>



</androidx.constraintlayout.widget.ConstraintLayout>